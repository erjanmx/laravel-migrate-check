{
    "name": "erjanmx/laravel-migrate-check",
    "description": "A command to check for pending migrations. Exits with non-zero code if there are any",
    "keywords": [
        "migrate",
        "check",
        "eloquent",
        "database",
        "laravel-migrate-check"
    ],
    "homepage": "https://github.com/erjanmx/laravel-migrate-check",
    "license": "MIT",
    "authors": [
        {
            "name": "Erjan",
            "email": "erjanmx@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0",
        "illuminate/events": "5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.6.* || 3.7.* || 3.8.* || 3.9.* || ^4.0 || ^5.0",
        "phpunit/phpunit": "^7.5 || ^8.4"
    },
    "autoload": {
        "psr-4": {
            "Erjanmx\\MigrateCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erjanmx\\MigrateCheck\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Erjanmx\\MigrateCheck\\MigrateCheckServiceProvider"
            ]
        }
    }
}
