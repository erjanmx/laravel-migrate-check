{
    "name": "erjanmx/laravel-migrate-check",
    "description": "A command to check for pending migrations. Exits with non-zero code if there are any",
    "keywords": [
        "migrate",
        "check",
        "eloquent",
        "database",
        "laravel-migrate-check"
    ],
    "homepage": "https://github.com/erjanmx/laravel-migrate-check",
    "license": "MIT",
    "authors": [
        {
            "name": "Erjan",
            "email": "erjanmx@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2||^8.0",
        "illuminate/console": "^6|^7|^8|^9|^10.0",
        "illuminate/events": "^6|^7|^8|^9|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Erjanmx\\MigrateCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erjanmx\\MigrateCheck\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Erjanmx\\MigrateCheck\\MigrateCheckServiceProvider"
            ]
        }
    }
}
